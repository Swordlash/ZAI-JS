  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.0.0",
    "parcel": "^2.0.1",
    "purescript": "^0.14.9",
    "purescript-language-server": "^0.16.6",
    "purescript-psa": "^0.8.2",
    "showdown": "^2.1.0",
    "spago": "^0.20.7"
  },
  "dependencies": {
    "big-integer": "^1.6.51"
  }
}
